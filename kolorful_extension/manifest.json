{
  "manifest_version": 2,

  "name": "Kolorful",
  "description": "Kolorful will analyze the current webpage's color distribution",
  "version": "1.0",

  "browser_action": {
  "default_icon": "Kolorful_icon.png",
  "default_popup": "popup.html"

  },

  "permissions": [
    "tabs"
  ],

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
      "js": ["contentv2_wip.js"]
    }
  ]



}
